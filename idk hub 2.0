        function fullUpdate()
                local updatedSettings = {
                        commandPrefix = commandPrefix;
                        hotkeys = hotkeys;
                        fchotkeymode = fchotkeymode;
                }
                local fullUPDATED = gsHttp:JSONEncode(updatedSettings)
