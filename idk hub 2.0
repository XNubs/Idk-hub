        function hasTools()
                local a = false
                local b = false
                for i,v in pairs(LP.Character:GetDescendants()) do
                        if v:IsA("Tool") then
                                if v ~= nil then
                                        a = true
                                else
                                        a = false
                                end
                        end
                end
                for i,k in pairs(LP.Backpack:GetDescendants()) do
                        if k:IsA("Tool") then
                                if k ~= nil then
                                        b = true
                                else
                                        b = false
                                end
                        end
                end
                return a or b
        end
